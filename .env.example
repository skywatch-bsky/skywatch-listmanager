# Bluesky Authentication
# Your Bluesky handle (e.g., user.bsky.social)
BSKY_HANDLE=

# Your Bluesky password or app password
BSKY_PASSWORD=

# DID of the account hosting the lists (e.g., did:plc:abc123...)
DID=

# Personal Data Server (usually bsky.social)
PDS=bsky.social

# WebSocket URL for the label firehose
# Example: wss://ozone.skywatch.blue/xrpc/com.atproto.label.subscribeLabels
WSS_URL=

# Redis connection URL (default works with docker-compose)
REDIS_URL=redis://redis:6379

# Optional: Logging level (debug, info, warn, error)
LOG_LEVEL=info
